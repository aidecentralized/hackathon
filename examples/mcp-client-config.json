{
  "mcpServers": {
    "starbucks": {
      "comment": "Configuration for Starbucks MCP server",
      "command": "node",
      "args": ["dist/index.js"],
      "env": {
        "PORT": "3000"
      }
    },
    "starbucks-remote": {
      "comment": "Example for connecting to deployed Starbucks server",
      "command": "npx",
      "args": ["mcp-remote", "https://your-starbucks-server.railway.app/mcp"]
    }
  }
} 